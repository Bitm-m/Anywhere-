<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <moe.shizuku.preference.PreferenceCategory
        app:key="normalCategory"
        app:title="@string/settings_category_normal">

        <SwitchPreferenceCompat
            app:key="transparentIcon"
            app:title="@string/settings_transparent_icon_title"
            app:summary="@string/settings_transparent_icon_summary"
            app:icon="@drawable/ic_transparent_icon"
            app:defaultValue="false" />

        <SwitchPreferenceCompat
            app:key="editorEntryAnim"
            app:title="@string/settings_editor_entry_animation"
            app:summary="@string/settings_editor_entry_animation_summary"
            app:icon="@drawable/ic_animation"
            app:defaultValue="true" />

    </moe.shizuku.preference.PreferenceCategory>

</PreferenceScreen>